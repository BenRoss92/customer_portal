<?php

namespace CustomerPortalBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="customer")
 */
class Customer
{
  /**
   * @ORM\Column(type="integer")
   * @ORM\Id
   * @ORM\GeneratedValue(strategy="AUTO")
   */
  private $id;

  /**
   * @ORM\Column(type="string")
   */
  private $name;

  /**
   * @ORM\Column(type="string")
   */
  private $address;

  /**
   * @ORM\Column(type="string")
   */
  private $city;

  /**
   * @ORM\Column(type="string")
   */
  private $country;
}
